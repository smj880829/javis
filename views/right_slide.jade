.right-slide(ng-show='chat_show')
  .chat(ng-controller='chatCtl')
    .chat_mid(ng-scrollable="{scrollX:'none'}")
      .ani-repeat(ng-repeat='log in chat_logs', align='{{log.ali}}')
        .log
          | {{log.message}}
      #chat_bottom
    .chat_bot
      input.form-control(type='text', ng-model='message', my-enter='insertmsg_angular()')
